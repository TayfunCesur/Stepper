<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Stepper">
        <attr name="duration" format="integer"/>
        <attr name="stepCount" format="integer"/>
        <attr name="useRTL" format="enum">
            <enum name="NO_RTL" value="0"/>
            <enum name="AUTO" value="1"/>
            <enum name="FORCE" value="2"/>
        </attr>
    </declare-styleable>
</resources>